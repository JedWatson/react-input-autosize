{
  "presets": [
    ["@babel/env", { "loose": true }],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/proposal-class-properties", { "loose": true }],
    ["transform-react-remove-prop-types", { "mode": "unsafe-wrap" }]
  ],
  "env": {
    "production": {
      "ignore": ["node-modules/**", "src/__tests__/**"],
    },
    "test": {
      "ignore": ["node-modules/**"],
    }
  }
}
